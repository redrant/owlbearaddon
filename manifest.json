{
  "name": "Dashisboard Plus",
  "version": "1.5.0",
  "manifest_version": 1,
  "action": {
    "title": "Dashisboard+",
    "icon": "https://www.owlbear.rodeo/favicon.ico",
    "popover": "/owlbearaddon/index.html",
    "width": 450,
    "height": 650
  },
  "permissions": [
    {
      "trigger": "onSceneReady",
      "action": "read"
    },
    {
      "trigger": "onMetadataChange",
      "action": "read"
    },
    {
      "target": "scene",
      "action": "write"
    }
  ]
}
