{
  "name": "Dashisboard+",
  "version": "1.0.2",
  "manifest_version": 1,
  "action": {
    "title": "Dashisboard+",
    "icon": "https://www.owlbear.rodeo/favicon.ico",
    "popover": "/index.html",
    "width": 400,
    "height": 600
  },
  "permissions": [
    {
      "trigger": "onSceneReady",
      "action": "read"
    },
    {
      "trigger": "onMetadataChange",
      "action": "read"
    }
  ]
}
